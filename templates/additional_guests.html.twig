{#
/**
 * @file additional_guests.html.twig
 * Default theme implementation to present Additional guests data.
 *
 * This template is used when viewing Additional guests pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_additional_guests()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('additional_guests') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
